#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ui369/Workspace/Projects/DAOMasons/code/piepay/piepay-backend/node_modules/.pnpm/rescript@11.1.3/node_modules/rescript/node_modules:/Users/ui369/Workspace/Projects/DAOMasons/code/piepay/piepay-backend/node_modules/.pnpm/rescript@11.1.3/node_modules:/Users/ui369/Workspace/Projects/DAOMasons/code/piepay/piepay-backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ui369/Workspace/Projects/DAOMasons/code/piepay/piepay-backend/node_modules/.pnpm/rescript@11.1.3/node_modules/rescript/node_modules:/Users/ui369/Workspace/Projects/DAOMasons/code/piepay/piepay-backend/node_modules/.pnpm/rescript@11.1.3/node_modules:/Users/ui369/Workspace/Projects/DAOMasons/code/piepay/piepay-backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bsc" "$@"
else
  exec node  "$basedir/../../bsc" "$@"
fi
